pipeline
{
    agent any
        stages
        {
            stage("Build")
            {
                steps
                {
                echo("Build the Project")
                }
            }
             stage("Dev ")
            {
                steps
                {
                echo("Deploy to Dev")
                }
            }
             stage("Deploy to QA")
            {
                steps
                {
                echo("QA Deployement")
                }
            }
             stage("Run Automation Test ")
            {
                steps
                {
                echo("Run Automation Tests")
                }
            }
             stage("UAT ")
            {
               steps
                {
                echo("Run UAT Tests")
                }
            }
            
        }
}